{
  "name": "contributte/latte-email",
  "description": "Latte email helper to protect your email addresses.",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/latte-email",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": "^7.2",
    "latte/latte": "~2.4.7",
    "nette/utils": "~2.4.9 || ~2.5.3"
  },
  "require-dev": {
    "nette/di": "~2.4.10",
    "ninjify/nunjuck": "^0.2.0",
    "ninjify/qa": "^0.4.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Latte\\Email\\Helpers\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.1-dev"
    }
  }
}
